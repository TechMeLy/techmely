{
  "name": "techmely-monorepo",
  "private": true,
  "license": "AGPLv3",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "nx run-many -t build --parallel 5",
    "build.deps": "nx run-many -t build --parallel 5 --exclude @techmely/web --exclude techmely-monorepo",
    "stub": "nx run-many -t stub",
    "dev": "nx run-many -t dev",
    "prepare": "husky install",
    "lint.format": "biome check . --apply",
    "lint.circular": "madge --extensions ts --exclude '.d.ts$' --circular .",
    "test.unit": "vitest --passWithNoTests",
    "test.unit.coverage": "vitest --coverage.all",
    "test.unit.run": "vitest run --passWithNoTests",
    "migration.add": "node scripts/generateMigrateFileApi.mjs",
    "migration.run": "tsx scripts/migrate-db.ts",
    "docker.build": "nx run-many -t docker.build",
    "docker.storage": "nx run-many -t docker.storage",
    "docker.up": "nx run-many -t docker.up"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.1",
    "@cucumber/cucumber": "10.1.0",
    "@playwright/test": "^1.40.1",
    "@squoosh/cli": "0.7.3",
    "@techmely/types": "1.5.2",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/fs-extra": "11.0.4",
    "@types/mixpanel-browser": "2.48.1",
    "@types/node": "20.10.6",
    "@types/nprogress": "0.2.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/ua-parser-js": "0.7.39",
    "@vitest/coverage-v8": "1.1.1",
    "concurrently": "8.2.2",
    "custom-element-jet-brains-integration": "1.4.1",
    "custom-element-vs-code-integration": "1.2.1",
    "dependency-cruiser": "16.0.0",
    "dotenv": "16.3.1",
    "esbuild": "0.19.11",
    "fast-glob": "3.3.2",
    "fs-extra": "11.2.0",
    "happy-dom": "12.10.3",
    "husky": "8.0.3",
    "knip": "3.9.0",
    "kysely": "0.27.0",
    "madge": "6.1.0",
    "msw": "2.0.11",
    "multiple-cucumber-html-reporter": "3.6.0",
    "nano-staged": "0.8.0",
    "nx": "17.2.8",
    "pino": "8.17.2",
    "sass": "1.69.6",
    "speedscope": "1.19.0",
    "syncpack": "12.3.0",
    "tasuku": "2.0.1",
    "ts-essentials": "9.4.1",
    "ts-lit-plugin": "2.0.1",
    "ts-node": "10.9.2",
    "tsarch": "5.3.0",
    "tslib": "2.6.2",
    "tsup": "8.0.1",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vitest": "1.1.1",
    "vitest-axe": "v1.0.0-pre.3",
    "zx": "7.2.3"
  },
  "engines": {
    "node": ">=20.x"
  },
  "nano-staged": {
    "*.{mjs,js,ts,tsx}": [
      "pnpm lint.format",
      "pnpm lint.circular"
    ]
  }
}
