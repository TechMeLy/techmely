{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../../.moon/cache/types/apps/web",
    "noEmit": true,
    "emitDeclarationOnly": false,
    "incremental": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "paths": {
      "#root/*": ["./*"]
    }
  },
  "exclude": ["node_modules"],
  "include": [
    "assets",
    "certs",
    "locales",
    "modules",
    "pages",
    "public",
    "server",
    "shared",
    "typings"
  ],
  "references": [
    {
      "path": "../../packages/design-system"
    }
  ]
}
