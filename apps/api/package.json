{
  "name": "@techmely/api",
  "version": "0.0.1",
  "private": true,

  "description": "Techmely API",
  "author": {
    "name": "Harry Tran",
    "email": "nhattq.coding@gmail.com",
    "url": "https://github.com/techmely"
  },
  "script": {
    "dev": "bun run --hot src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build --target=bun --minify --outdir=./dist src/index.ts && bun build --target=bun --minify --outdir=./dist migrate.ts",
    "migrate:dev:latest": "bun run ./migrate.ts dev latest"
  },
  "dependencies": {
    "@techmely/utils": "^3.0.0",
    "kysely": "0.27.3",
    "nanoid": "^5.0.7",
    "openapi-typescript": "^6.7.5",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@techmely/types": "1.6.6"
  }
}
